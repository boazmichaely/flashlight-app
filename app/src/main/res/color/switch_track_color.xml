<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Light blue track when switch is checked/ON -->
    <item android:state_checked="true" android:color="@color/toggle_on_track" />
    <!-- Gray track when switch is unchecked/OFF -->
    <item android:color="@color/toggle_off_track" />
</selector>



